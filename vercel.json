{
  "cleanUrls": true,
"rewrites": [
  { "source": "/", "destination": "/index.html" },
  { "source": "/profile", "destination": "/profile.html" },
  { "source": "/about", "destination": "/about.html" },
  { "source": "/chat", "destination": "/chat.html" },
  { "source": "/blog", "destination": "/blog.html" },
  { "source": "/docs", "destination": "/docs.html" },
  { "source": "/register", "destination": "/register.html" },
  { "source": "/login", "destination": "/login.html" },

  { "source": "/favicon.ico", "destination": "/static/images/favicon.ico" },
  { "source": "/manifest.json", "destination": "/static/manifest.json" },
  { "source": "/robots.txt", "destination": "/static/robots.txt" },
  { "source": "/sitemap.xml", "destination": "/static/sitemap.xml" }
]
,
  "redirects": [
    { "source": "/home", "destination": "/", "permanent": true }
  ],
"headers": [
  {
    "source": "/(.*)",
    "headers": [
      { "key": "X-Content-Type-Options", "value": "nosniff" },
      { "key": "X-Frame-Options", "value": "DENY" },
      { "key": "X-XSS-Protection", "value": "1; mode=block" }
    ]
  },
  {
    "source": "/sitemap.xml",
    "headers": [
      { "key": "Content-Type", "value": "application/xml" }
    ]
  }
]

}
